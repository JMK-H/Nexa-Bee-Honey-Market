	var sCategoryName = this.ds_menu.lookup("MENU_ID", sCategoryId, "MENU_NAME");
	var sStatus = this.dsCurrentItem.getColumn(nRow, "POST_STATUS");
	var sBuyerId = this.dsCurrentItem.getColumn(nRow, "BUYER_ID");
	var sLikeCnt = this.dsCurrentItem.getColumn(nRow, "LIKE_CNT");
	var sUnlikeCnt = this.dsCurrentItem.getColumn(nRow, "UNLIKE_CNT");
	var sMyLikeState = this.dsCurrentItem.getColumn(nRow, "MY_LIKE_STATE");
	
	var sPopupId = "sDetail"; //Popup ID
	var sUrl = "Main::ggul1300pu.xfdl"; //팝업 URL
	var oArg = {sTitle:sTitle, sImageUrl:sImageUrl, sDate:sDate, sPrice:sPrice, sWriter:sWriter,
				sCommend:sCommend, sCategoryName:sCategoryName, sStatus:sStatus, sBuyerId:sBuyerId, sPostId:sPostId
				, sLikeCnt:sLikeCnt, sUnlikeCnt:sUnlikeCnt, sMyLikeState:sMyLikeState}; //전달값
	var sPopupCallback = "fnCallPopup";
	var oOption = {title:"모달 팝업", titlebar: false}; //모달리스가 아니라 모달이면 popuptupe 지우기
	this.gfnOpenPopup(sPopupId, sUrl, oArg, sPopupCallback, oOption);
};
//=========================================================카테고리 테그==============================================
this.fn_UpdateCategoryTag = function()
{
    var objApp = nexacro.getApplication();
    var sMenuId = objApp.gv_MenuId;

    if (this.gfnIsNull(sMenuId)) 
    {
        this.btnCategoryTag.set_visible(false);
		document.title = "꿀꺼억 - " + "검색";
        return;
    }

    var sMenuName = this.ds_menu.lookup("MENU_ID", sMenuId, "MENU_NAME");
    
    if (this.gfnIsNull(sMenuName)) sMenuName = "카테고리"; 
	document.title = "꿀꺼억 - " + sMenuName;
    this.btnCategoryTag.set_text("# " + sMenuName + "  ✖");
    this.btnCategoryTag.set_visible(true);
    this.btnCategoryTag.set_width(sMenuName.length * 15 + 40); 
};

this.btnCategoryTag_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
    var objApp = nexacro.getApplication();
    objApp.gv_MenuId = "";
    
    this.btnCategoryTag.set_visible(false);
    
    this.fnPageInit();
	document.title = "꿀꺼억 - " + "검색";
};
//==========================================콜백함수====================================================================
this.fnCallback = function (svcID, errCd, errMsg)
{
	if(svcID=="selectSearchPaging")
	{
		this.fnPagingSetting();
		this.fn_SetResultData();
		this.drawZzimBtn(this.fvRecords);
		this.staResultTitle.text = "'"+nexacro.getApplication().gv_Search+"'"+" 검색 결과"
		if(nexacro.getApplication().gv_Search==undefined)
		{
			nexacro.getApplication().gv_Search= "";
			this.staResultTitle.text = "'"+nexacro.getApplication().gv_Search+"'"+" 검색 결과"
		}
		
        var nRowCount = this.dsCurrentItem.getRowCount();

        if (nRowCount == 0) 
        {
            this.divList.set_visible(false);
            
            this.staNoData.set_visible(true);
        } 
        else 
        {
            // [데이터 있음]
            this.divList.set_visible(true);
            this.staNoData.set_visible(false);
        }
		
	}
	else if(svcID=="getMenuList")
	{
		this.fn_UpdateCategoryTag();
	}
};

this.fnPagingCallback = function(nPage, nRecordsOffset)
{
	this.fvPage = nPage; 				
	this.fvRecordsOffset = nRecordsOffset;
	
	this.fnSearch(); //조회함수호출
};
//==============================================팝업 콜백==========================================================

this.fnCallPopup = function (svcID, rtn)
{
   
	if(svcID == "sDetail")
	{
		this.fnSearch();
	}

};]]></Script>
