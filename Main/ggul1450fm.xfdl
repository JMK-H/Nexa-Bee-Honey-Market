// 버튼 스타일 관리 함수
this.fn_setSortButtonStyle = function(objSelectedBtn)
{

    var arrBtns = [
        this.divFilter.form.btnSortAsc,
        this.divFilter.form.btnSortDesc
    ];
    
    for(var i=0; i<arrBtns.length; i++) {
        arrBtns[i].set_background("#ffffff");          
